CREATE TABLE  "INSTITUTE_DETAILS" 
   (	"INSTITUTENAME" VARCHAR2(100) NOT NULL ENABLE, 
	"INSTITUTEID" VARCHAR2(100) NOT NULL ENABLE, 
	"TOTALSEAT" NUMBER NOT NULL ENABLE, 
