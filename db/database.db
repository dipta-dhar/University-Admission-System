	 PRIMARY KEY ("INSTITUTEID") ENABLE, 
	 UNIQUE ("INSTITUTENAME", "INSTITUTEID") ENABLE
   )
/
CREATE TABLE  "SEAT_ROOM_DETAILS" 
   (	"INSTITUTEID" VARCHAR2(100) NOT NULL ENABLE, 
	"BUILDINGNAME" VARCHAR2(100) NOT NULL ENABLE, 
	"FLOOR" VARCHAR2(50) NOT NULL ENABLE, 
	"ROOMNO" VARCHAR2(50) NOT NULL ENABLE, 
	"SEAT" NUMBER NOT NULL ENABLE, 
	 UNIQUE ("BUILDINGNAME", "INSTITUTEID", "FLOOR", "ROOMNO") ENABLE, 
	 FOREIGN KEY ("INSTITUTEID")
	  REFERENCES  "INSTITUTE_DETAILS" ("INSTITUTEID") ENABLE
   )
/